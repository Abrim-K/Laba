#include <iostream>
using namespace std;

int main() {
	setlocale(LC_ALL, "RU");
	int n;
	cout << "Введите количество элементов массива (1-100): ";
	cin >> n;
	int mas[100];
	cout << "Введите элементы массива (со значениями от 1 до 100):" << endl;
	for (int i = 0; i < n; ++i) {
		cin >> mas[i];
	}
	cout << "четные элементы:";
	for (int i = 0; i < n; ++i) {
		if ((mas[i] % 2) == 0) {
			cout << mas[i] << " ";
		}
	}
	return 0;
}
