#include <iostream>
#include <stack>
using namespace std;

stack<int> stak;
stack<int> stakrev;

int main() {
	setlocale(LC_ALL, "RU");
	int s;
	int proiz = 1;
	cout << "введите целые значения стека (для окончания ввода введите 0)" << endl;
	cin >> s;
	while (s != 0) {
		stak.push(s);
		if (abs(s % 2) == 1) {
			proiz *= s;
		}
		cin >> s;
	}
	cout << "произведение: " << proiz << endl;
	cout << "Данные стека: ";
	while (!stak.empty()) {
		cout << stak.top() << "\t";
		stak.pop();
	}
	return 0;
}
